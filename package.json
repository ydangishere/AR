    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "svgo": "^4.0.0",
    "svgstore": "^3.0.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
